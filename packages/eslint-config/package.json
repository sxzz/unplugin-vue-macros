{
  "name": "@vue-macros/eslint-config",
  "version": "0.7.0",
  "packageManager": "pnpm@9.12.3",
  "description": "eslintConfig feature from Vue Macros.",
  "type": "module",
  "keywords": [
    "vue-macros",
    "macros",
    "vue",
    "sfc",
    "setup",
    "script-setup",
    "eslint-config"
  ],
  "license": "MIT",
  "homepage": "https://vue-macros.dev",
  "bugs": {
    "url": "https://github.com/vue-macros/vue-macros/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-macros/vue-macros.git",
    "directory": "packages/eslint-config"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./flat": {
      "dev": "./src/flat.ts",
      "require": "./dist/flat.cjs",
      "import": "./dist/flat.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.js"
      },
      "./flat": {
        "require": "./dist/flat.cjs",
        "import": "./dist/flat.js"
      },
      "./*": "./*"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "DEV=true tsup"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "eslint": "^9.14.0"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
